{
  "name": "tipibot-controller",
  "version": "1.0.0",
  "description": "A controller for the tipibot",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "install": "electron-rebuild",
    "pack": "electron-packager . tipibot-controller --platform=darwin --arch=arm64",
    "pack:win": "electron-packager . tipibot-controller --platform=win32 --arch=x64",
    "pack:arm7l": "electron-packager . tipibot-controller --platform=linux --arch=armv7l"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [],
  "author": "IDLV - Arthur Masson",
  "license": "MIT",
  "devDependencies": {
    "electron": "^30.1.0",
    "electron-builder": "^25.0.0-alpha.9",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "jquery": "^3.7.1",
    "paper": "^0.12.17",
    "perspective-transform": "^1.1.3",
    "serialport": "^9.2.8",
    "socketio": "^1.0.0",
    "three": "^0.165.0",
    "three.meshline": "^1.4.0",
    "ws": "^8.17.0"
  },
  "build": {
    "appId": "tipibot-controller",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "asar": false
    }
  }
}
